<input
    id="<%= id %>"
    name="<%= attrName %>"
    type="<%= type %>"
    data-type="<%= type %>"
    data-label="<%= label %>"
    class="<%= classes %>"
    <% if(typeof maxlength === 'number') { %>maxlength="<%= maxlength %>"<% } %>
    <%== required ? 'aria-required="true"' : '' %>
    value="<%= model.attr( attrName ) ? model.attr( attrName ) : '' %>"
    placeholder="<%= this.placeholder && typeof this.placeholder === 'string' ? this.placeholder : '' %>"
    <%= (el) -> el.tooltip( tooltipOptions ) %>
    <% if( !disabled.attr( attrName + '()' ) && disabled.attr( '**()' ) === false ) {
        %>disabled="disabled"<%
    } %>
/>