<input
    id="<%= id %>"
    name="<%= attrName %>"
    type="checkbox"
    data-type="<%= type %>"
    data-label="<%= label %>"
    class="<%= classes %>"
    <%== required ? 'aria-required="true"': '' %>
    <%== model.attr( attrName ) ? 'checked="checked"' : '' %>
    <%= (el) -> el.tooltip( tooltipOptions ) %>
    <% if( !disabled.attr( attrName + '()' ) && disabled.attr( '**()' ) === false ) {
        %>disabled="disabled"<%
    } %>
/>